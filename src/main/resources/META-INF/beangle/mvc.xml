<?xml version="1.0"?>
<mvc>
  <profile name="openurp-edu-course-webapp" package="org.openurp.edu.course.web.action">
    <url path="/" suffix=""  style="seo" />
    <interceptors>
      <interceptor name="web.Interceptor.hibernate"/>
      <interceptor name="web.Interceptor.security"/>
    </interceptors>
  </profile>
  <profile name="openurp-edu-course-ws" package="org.openurp.edu.course.web.ws">
    <action suffix="WS" defaultMethod="index" />
    <url path="/" suffix=""  style="seo" />
    <interceptors>
      <interceptor name="web.Interceptor.hibernate"/>
    </interceptors>
  </profile>
</mvc>

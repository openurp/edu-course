
[#macro info_header title]
<div class="content-header" style="padding-bottom: 0px;">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h5 class="m-0 text-dark">课程信息 <small>${title}</small></h5>
      </div><!-- /.col -->
      <div class="col-sm-6">
        <ol class="breadcrumb  float-sm-right">
          <li class="breadcrumb-item"><i class="fas fa-tachometer-alt" style="margin-top: 6px;"></i>课程信息</li>
          <li class="breadcrumb-item active">${title}</li>
        </ol>
      </div><!-- /.col -->
    </div><!-- /.row -->
  </div>
</div>
[#----]
[/#macro]
